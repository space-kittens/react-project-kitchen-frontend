{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard",
    "prettier"
  ],
  "plugins": ["babel" ,"import", "jsx-a11y", "react", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    // добавленно временно
    "react/prop-types": "off",
    "react/no-deprecated": "off"
  }
}
